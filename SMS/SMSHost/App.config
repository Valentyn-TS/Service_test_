<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
	<system.serviceModel>
		<services>
   <service behaviorConfiguration="mexBehavior" name="SMS.MessageServ">
    <endpoint address="SMS.MesageServ" binding="basicHttpBinding"
     contract="SMS.IMesageServ" />
    <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
    <host>
     <baseAddresses>
      <add baseAddress="localhost:41567" />
     </baseAddresses>
    </host>
   </service>
  </services>
		<behaviors>
			<serviceBehaviors>
				<behavior name="mexBehavior" >
					<serviceMetadata httpGetEnabled="true"/>
				</behavior>
			</serviceBehaviors>
		</behaviors>
	</system.serviceModel>
		
</configuration>