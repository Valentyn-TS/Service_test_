<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
		<services>
			<service name="Server.MultiplyService" behaviorConfiguration="mexBehavior">
				<endpoint address="MultiplyService" binding="basicHttpBinding" contract="Server.IMultiplyService"> </endpoint>
				<endpoint address="MultiplyService" binding="netTcpBinding" contract="Server.IMultiplyService"> </endpoint>
				<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"> </endpoint>
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8080"/>
						<add baseAddress="net.tcp://localhost:2023"/>
					</baseAddresses>
				</host>
			</service>
		</services>
		<behaviors>
			<serviceBehaviors>
				<behavior name ="mexBehavior"> 
					<serviceMetadata httpGetEnabled="true"></serviceMetadata>
				</behavior>
			</serviceBehaviors>
		</behaviors>
	</system.serviceModel>
</configuration>